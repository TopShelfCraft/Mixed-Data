{
    "name": "topshelfcraft/mixed-data",
    "description": "A simple JSON-backed field for storing mixed data.",
    "type": "craft-plugin",
    "version": "3.0.0-beta.1",
    "keywords": [
        "craft",
        "craftcms",
        "craft-plugin",
        "mixed",
        "json"
    ],
    "support": {
        "issues": "https://github.com/topshelfcraft/mixed/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Top Shelf Craft (Michael Rog)",
            "homepage": "https://topshelfcraft.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "craftcms/cms": "^3.2.0"
    },
    "autoload": {
        "psr-4": {
          "topshelfcraft\\MixedData\\": "src/"
        }
    },
    "extra": {
        "class": "topshelfcraft\\MixedData\\MixedData",
        "handle": "mixed",
        "name": "Mixed Data"
    }
}
