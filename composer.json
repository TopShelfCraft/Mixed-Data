{
    "name": "topshelfcraft/mixed-data",
    "description": "A simple JSON-backed field for storing mixed data.",
    "type": "craft-plugin",
    "version": "4.0.0-beta.1",
    "keywords": [
        "craft",
        "craftcms",
        "craft-plugin",
        "mixed",
        "data",
        "json"
    ],
    "support": {
        "issues": "https://github.com/TopShelfCraft/Mixed-Data/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Top Shelf Craft (Michael Rog)",
            "homepage": "https://topshelfcraft.com"
        }
    ],
    "require": {
        "craftcms/cms": "^4.2"
    },
    "autoload": {
        "psr-4": {
          "TopShelfCraft\\MixedData\\": "src/"
        }
    },
    "extra": {
        "class": "TopShelfCraft\\MixedData\\MixedData",
        "handle": "mixed-data",
        "name": "Mixed Data (JSON-backed fields)"
    }
}
